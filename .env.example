# API URL - URL for your backend API
VITE_API_URL=http://localhost:5000/api

# Google OAuth Client ID
# Get this from the Google Cloud Console (https://console.cloud.google.com)
# Under "APIs & Services" > "Credentials" > "Create OAuth client ID"
# Copy the Client ID here
VITE_GOOGLE_CLIENT_ID=your-client-id-goes-here.apps.googleusercontent.com

# Environment indicator
VITE_ENV=development

# Enable or disable detailed logging (true/false)
VITE_ENABLE_LOGGING=true